# SENSE HAT SERVICES
# ===================

server {
    listen 8086;
    server_name 192.168.0.10 127.0.0.1;
    root /home/pi/sensehat-datalog;
    index index.html;

    location /sensedata {
        proxy_set_header Host $host;
        proxy_pass http://127.0.0.1:1880;
    }

}

server {
    listen 8088;
    server_name 192.168.0.10 127.0.0.1;
    root /home/pi/sensehat-dataviewer;
    index index.html;

    location / {
        allow all;
    }

}

